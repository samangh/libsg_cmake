function(get_processor RETURN_VARIABLE_NAME)
  if(CMAKE_SYSTEM_PROCESSOR MATCHES "(x86)|(X86)|(amd64)|(AMD64)")
    set (${RETURN_VARIABLE_NAME} "X86" PARENT_SCOPE)
  elseif(CMAKE_SYSTEM_PROCESSOR MATCHES ".*(arm|ARM).*")
    set (${RETURN_VARIABLE_NAME} "ARM" PARENT_SCOPE)
  else()
    set (${RETURN_VARIABLE_NAME} "UNKNOWN" PARENT_SCOPE)
  endif ()
endfunction()
